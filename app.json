{
  "expo": {
    "name": "rewrite_app",
    "slug": "rewrite_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/glogo.png",
    "scheme": "rewriteapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundlerIdentifier": "com.rewriteapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/glogo.png",
        "backgroundImage": "./assets/images/glogo.png",
        "monochromeImage": "./assets/images/glogo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.rewriteapp"
    },
    "web": {
      "output": "server",
      "favicon": "./assets/images/glogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/glogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "872ef885-a3ff-476f-86d7-cc107da84c22"
      }
    }
  }
}
